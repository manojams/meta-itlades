NAME=prognand
LOGFILE=/home/root/log/$NAME.log

start() {
  mkdir /home/root/log
  echo 'Starting serviceâ€¦' >&2
  /home/root/flash_nand/prognand_sboot.sh &> $LOGFILE
}


case "$1" in
  start)
    start
    ;;
  stop)
    stop
    ;;
  uninstall)
    uninstall
    ;;
  retart)
    stop
    start
    ;;
  *)
    echo "Usage: $0 {start|stop|restart|uninstall}"
esac
